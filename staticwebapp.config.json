{
  "routes": [
    {
      "route": "/login",
      "redirect": "/.auth/login/aad"
    },
    {
      "route": "/b2c",
      "redirect": "/.auth/login/aad"
    }
  ],
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "projectPackAAD": {
          "registration": {
            "clientIdSettingName": "clientId",
            "clientCredential": {
              "clientSecretSettingName": "secret"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://msapiuat.b2clogin.com/tfp/msapiuat.onmicrosoft.com/B2C_1A_signup_signin/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": ["openid"]
          }
        }
      }
    }
  }
}
